{
  "name": "@jinshub/url-fallback",
  "version": "1.0.0-beta4",
  "description": "A tiny piece of JavaScript code which allows websites to set fallback urls for their resources.",
  "main": "dist/url-fallback.js",
  "module": "dist/url-fallback.esm.js",
  "types": "dist/url-fallback.d.ts",
  "unpkg": "dist/url-fallback.iife.min.js",
  "jsdelivr": "dist/url-fallback.iife.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build:watch",
    "build": "rimraf dist && rollup -c",
    "build:watch": "rollup -c -w",
    "prepublishOnly": "npm run test && npm run build",
    "preexamples": "npm run build && rimraf examples/dist && cp -r dist examples/dist",
    "examples": "http-server examples -a 127.0.0.1 -p 9999",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e:open": "concurrently \"npm run examples\" \"npm run cy:open\"",
    "e2e": "concurrently -k \"npm run examples\" \"npm run cy:run\"",
    "jest": "jest src",
    "test": "npm run jest && npm run e2e"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "browserslist": "> 0.25%, not dead",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huajiejin/url-fallback.git"
  },
  "keywords": [
    "cdn",
    "url",
    "fallback"
  ],
  "author": "Huajie (Jay) Jin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huajiejin/url-fallback/issues"
  },
  "homepage": "https://github.com/huajiejin/url-fallback#readme",
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@rollup/plugin-babel": "5.3.1",
    "@types/jest": "27.4.1",
    "concurrently": "7.1.0",
    "cypress": "9.5.3",
    "esbuild": "0.14.27",
    "http-server": "14.1.0",
    "jest": "27.5.1",
    "rimraf": "3.0.2",
    "rollup": "2.70.1",
    "rollup-plugin-dts": "4.2.0",
    "rollup-plugin-esbuild": "4.8.2",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  }
}
